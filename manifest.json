{
  "manifest_version": 2,
  "name": "Whatsapp Speed Audio",
  "description": "Change audio speed.",
  "version": "0.1",
  "browser_action": {
    "default_title": "Whatsapp Speed Audio",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}